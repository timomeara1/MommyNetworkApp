<h1>Listing moms</h1>

<table>
  <tr>
    <th>User</th>
    <th>Email</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @moms.each do |mom| %>
  <tr>
    <td><%= mom.user %></td>
    <td><%= mom.email %></td>
    <td><%= link_to 'Show', mom %></td>
    <td><%= link_to 'Edit', edit_mom_path(mom) %></td>
    <td><%= link_to 'Destroy', mom, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Mom', new_mom_path %>
